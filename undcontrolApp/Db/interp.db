record(waveform, "$(Sys)$(Dev)Val:Table-Wfrm"){
  field(DESC, "Interpolation Waveform")
  field(FTVL, "DOUBLE")
  field(NELM, "$(N)")
  info(autosaveFields, "VAL")
}

record(ao, "$(Sys)$(Dev)$(In)"){
  field(DESC, "Input Value")
  field(DOL,  "$(DOL) CP")
  field(FLNK, "$(Sys)$(Dev)interp_")
  #field(OMSL, "closed_loop")
}

record(aSub, "$(Sys)$(Dev)interp_"){
  field(SNAM, "interp1d")
  field(TPRO, "1")
  field(BSRV, "INVALID")

  field(FTA,  "DOUBLE")
  field(NOA,  1)
  field(INPA, "$(Sys)$(Dev)$(In) NPP")

  field(FTE,  "DOUBLE")
  field(NOE,  "$(N)")
  field(INPE, "$(Sys)$(Dev)Val:Table-Wfrm PP")

  field(FTVA, "DOUBLE")
  field(NOVA, "1")
  field(OUTA, "$(Sys)$(Dev)$(Out) PP MS")
}

record(ao, "$(Sys)$(Dev)$(Out)"){
  field(DESC, "Output Value")
  field(TPRO, "1")
  field(OUT,  "$(Sys)$(Dev)$(Out)1")
  field(FLNK, "$(Sys)$(Dev)$(Out)1")
}

record(ao, "$(Sys)$(Dev)$(Out)1"){
  field(DESC, "Output Value")
  field(TPRO, "1")
}
